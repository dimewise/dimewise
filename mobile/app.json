{
  "expo": {
    "name": "Dimewise",
    "slug": "dimewise",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon/dw_ios_app_icon_1024x1024.png",
    "scheme": "dimewise",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "splash": {
        "image": "./assets/icon/dw_splash_screen_1024x1024.png",
        "imageWidth": 200,
        "resizeMode": "contain",
        "backgroundColor": "#00EF90"
      },
      "bundleIdentifier": "com.anonymous.dimewise"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon/dw_ios_app_icon_1024x1024.png",
        "backgroundColor": "#00EF98"
      },
      "splash": {
        "image": "./assets/icon/dw_splash_screen_1024x1024.png",
        "imageWidth": 200,
        "resizeMode": "contain",
        "backgroundColor": "#00EF90"
      },
      "package": "com.anonymous.dimewise"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icon/dw_splash_screen_1024x1024.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#00EF90"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
