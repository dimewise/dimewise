{
	"name": "dimewise",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "bunx --bun vite",
		"build": "tsc && vite build",
		"check": "bunx @biomejs/biome check --write .",
		"preview": "vite preview",
		"init:env": "chmod +x ./scripts/init.sh && ./scripts/init.sh",
		"gen:db": "bunx supabase gen types typescript --local > src/types/supabase.ts",
		"db:reset-local": "bunx supabase db reset",
		"db:diff-local": "bunx supabase db diff --local",
		"db:lint-local": "bunx supabase db lint --local",
		"db:start": "bunx supabase start",
		"db:stop": "bunx supabase stop",
		"db:status": "bunx supabase status",
		"db:migration-new": "bunx supabase migration new",
		"db:migration-list-local": "bunx supabase migration list --local",
		"db:migration-up-local": "bunx supabase migration up --local"
	},
	"dependencies": {
		"@heroicons/react": "^2.1.4",
		"@hookform/resolvers": "^3.6.0",
		"@reduxjs/toolkit": "^2.2.5",
		"@supabase/auth-ui-react": "^0.4.7",
		"@supabase/auth-ui-shared": "^0.1.8",
		"@supabase/supabase-js": "^2.43.4",
		"bun": "^1.1.13",
		"i18next": "^23.11.5",
		"i18next-browser-languagedetector": "^8.0.0",
		"react": "^18.3.1",
		"react-daisyui": "^5.0.0",
		"react-dom": "^18.3.1",
		"react-hook-form": "^7.52.0",
		"react-i18next": "^14.1.2",
		"react-redux": "^9.1.2",
		"react-router-dom": "^6.23.1",
		"redux-persist": "^6.0.0",
		"supabase": "^1.176.10",
		"yup": "^1.4.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.0",
		"@tailwindcss/typography": "^0.5.13",
		"@types/bun": "latest",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react-swc": "^3.7.0",
		"autoprefixer": "^10.4.19",
		"daisyui": "^4.12.2",
		"postcss": "^8.4.38",
		"tailwindcss": "^3.4.4",
		"typescript": "^5.4.5",
		"vite": "^5.3.1"
	}
}
